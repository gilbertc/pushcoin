<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<!-- Drop Shadow Stack -->
<item>
	<shape>
		<size android:width="1px" />
		<solid android:color="#d7d7d7" />
	</shape>
</item>
<item>
	<shape>
		<size android:width="4px" />
		<solid android:color="#d2d2d2" />
	</shape>
</item>
<item>
	<shape>
		<size android:width="9px" />
		<gradient
			android:startColor="#e7e7e7"
			android:endColor="#aeaeae"
			android:angle="180" />
	</shape>
</item>
</layer-list>
